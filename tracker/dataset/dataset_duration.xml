<!--
    This dataset provides a list of execution times for a dataset SQL statement.
    It limits the durations retrieved to the last 1000.

    First REST parameter is the application name (optional)
    Second REST parameter is the dataset (or plugin) name (optional)
  -->
<dataset read="*" write="">
    <select>
    <![CDATA[
        SELECT 
            duration_ms as d
        FROM 
            request 
        WHERE
            ({$1} IS NULL OR {$1} = app_name)
            AND ({$2} IS NULL OR {$2} = dataset)
    ]]>
    </select>
</dataset>

