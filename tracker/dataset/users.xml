<!--
     Provides a list of users that have been seen by the tracker -
     this is not necessarily the same as the current possible user list.

     First REST parameter is the application name (optional).
  -->
<dataset read="*" write="">
    <select>
        SELECT DISTINCT 
            username
        FROM
            request
        WHERE
            ({$1} IS NULL OR app_name LIKE '%' || {$1} || '%')
        ORDER BY
            username ASC
    </select>
</dataset>
